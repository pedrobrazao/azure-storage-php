{% extends "base.html.twig" %}

{% block title %}Upload Blob{% endblock %}

{% block content %}
    <h1>Upload Blob</h1>

    {% if error %}
    <p class="error">ERROR: {{ error }}</p>
    {% endif %}
    <form method="POST" enctype="multipart/form-data">
    <dl>
    <dt>Container Name</dt>
    <dd>{{ container }}</dd>
    <dt>Blob File</dt>
    <dd>
    <input type="file" name="blob" title="Blob File">
    </dd>
    <dd>
    <button type="submit">Upload</button>
    </dd>
    </dl>
    </form>
{% endblock %}
